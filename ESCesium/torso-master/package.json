{
  "name": "typescriptest",
  "version": "1.0.2",
  "description": "some typescript build exp",
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "files": [
    "src/*"
  ],
  "directories": {},
  "dependencies": {
    "cesium": "^1.87.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "babel-jest": "^27.3.1",
    "concurrently": "^6.4.0",
    "eslint": "^8.3.0",
    "eslint-config-mdcs": "^5.0.0",
    "eslint-plugin-jest": "^25.2.4",
    "jest": "^27.3.1",
    "jest-cli": "^27.3.1",
    "parcel-bundler": "^1.12.4",
    "static-server": "^2.2.1",
    "three": ">=0.134.0",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "start": "concurrently \"parcel watch example/*.html --out-dir ./example/dev-bundle/ --public-url . --no-cache\" \"static-server\"",
    "build-examples": "parcel build --no-content-hash example/*.html --out-dir ./example/bundle/ --public-url . --no-cache",
    "lint": "eslint \"src/**/*.js\" \"test/**/*.js\" \"example/*.js\" && tsc -p tsconfig.json --noEmit",
    "test": "jest"
  },
  "keywords": [
    "typescript",
    "typings"
  ],
  "author": "wooz",
  "license": "MIT"
}
